#include<iostream>
using namespace std;

int main()
{
    string str, rev = "";
    
    cout << "Enter a string: ";
    cin >> str;
    
    // Reverse the String 
    for(int i=str.length() - 1; i>=0; i--)
    {
        rev += str[i];
    }

    if (str==rev)
    {
        cout << "This string is a palidrome." << endl;
    }
    else 
    {
        cout << "This string is not palidrome." << endl;
    }
    
    return 0;
    
}
